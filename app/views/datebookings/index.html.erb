<p id="notice"><%= notice %></p>

<h1>Datebookings</h1>

<table>
  <thead>
    <tr>
      <th>Date</th>
      <th>Etat</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @datebookings.each do |datebooking| %>
      <tr>
        <td><%= datebooking.date %></td>
        <td><%= datebooking.etat %></td>
        <td><%= link_to 'Show', datebooking %></td>
        <td><%= link_to 'Edit', edit_datebooking_path(datebooking) %></td>
        <td><%= link_to 'Destroy', datebooking, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Datebooking', new_datebooking_path %>
